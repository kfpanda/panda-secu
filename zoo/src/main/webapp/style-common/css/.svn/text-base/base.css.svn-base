.mes-talk-con .mestalk-list{border:1px solid #e6e6e6; padding:20px; background:#fafafa; position:relative;}
.mes-talk-con .mestalk-list .mes-talk-ico{position:absolute; left:-6px; top:10px; background:url(../images/mes-talk-ico1.gif) no-repeat center center; width:6px; height:9px;}
.mes-talk-con2 .mestalk-list{border:1px solid #d3e4f7; background:#f2f8ff;}
.mes-talk-con2 .mestalk-list .mes-talk-ico{left:inherit; right:-6px; background:url(../images/mes-talk-ico2.gif) no-repeat center center;}

.learn-pro{height:2px;}
.myplan-list dl{border-bottom:1px solid #ddd; padding-bottom:15px;}
.myplan-list dl:last-child{border-bottom:none;}
.myplan-list dt span{width:16px; height:16px; border-radius:8px; background:#1660b7; color:#fff; line-height:16px; text-align:center;}
.myplan-list dt a{margin:0 10px;}
.myplan-list .mesplanlist-tit{width:180px;}
.myplan-list .mesplanlist-tit, .myplan-list .mesplanlist-tit a{color:#555; font:14px/30px microsoft yahei;}

.popup-img button{position:absolute; right:0; opacity:1; color:#fff; background:#000; width:30px; height:30px; text-shadow:none; display:none;}
.popup-img .modal-content:hover button{color:#fff; background:#000; opacity:.5; display:block;}
.popup-img button span{display:block; margin-top:-3px;}
.popup-img img{max-width:100%; margin:0 auto;}



/** 积分 **/
.mypoint{}
.mypoint .cy-listtab .nav-tabs>li>a{border:0px solid #fff;}
.mypoint .cy-listtab .nav-tabs>li>a:hover{color: #333;}
.mypoint .alert-info {border: none;background-color:#eff5fb;color: #6291c9;border-radius:0px;padding: 8px 13px;}

/** 积分-表格 **/
.mypoint .table { font-size: 16px;color: #666;}
.mypoint .table>tbody>tr>td{ padding: 20px 8px;text-align: center;}
.mypoint .learningTasks .table>tbody>tr>td{ padding: 25px 8px 15px 8px;text-align: center;}
/*.mypoint .table>tbody>tr>td:first-child{text-align: left;}*/
.mypoint .table>tbody>tr:first-child>td{ border-top: none;}
.mypoint .learningTasks .table>tbody>tr>td{ text-align: left;}
.mypoint .learningTasks .table>tbody>tr>td:last-child{ text-align: right;}
.mypoint .table>tbody>tr>td:last-child>button{ width: 100px;}
.mypoint .table>tbody>tr.completed:not(button){ color: #999999;}
.mypoint .table>tbody>tr.completed button{ cursor: not-allowed; filter: alpha(opacity=65); -webkit-box-shadow: none; box-shadow: none; opacity: .65; background-color: #a2a2a2;border: none;}
.mypoint .table>thead>tr>th{ padding: 10px 8px 10px 8px;background-color: #f4f4f4;color: #333333;text-align: center;border: 2px solid #FFFFFF}
.mypoint .tab-pane>p{ text-indent: 2em;font-size: 16px;color: #666666;line-height: 30px;}
.mypoint .tab-pane>p.head{font-weight: bold;color: #333333;}

/** 积分-新手向导模拟框 **/
.mypointModal .modal-content{border-radius: 10px;}
.mypointModal .modal-dialog{min-width: 300px;}
@media (min-width: 768px){ .mypointModal .modal-dialog { width: 450px;}}

/** 积分-新手向导图片轮播 **/
.mypointModal .carousel {margin-top:40px;}
.mypointModal .carousel .carousel-inner .item{min-height: 400px;}
.mypointModal .carousel .carousel-inner .item>img{margin: auto;}
.mypointModal .carousel .carousel-indicators .active{background-color: #adadad;}
.mypointModal .carousel .carousel-indicators li{border: 0px solid #fff;background-color: #e2e2e2;}
@media screen and (min-width: 768px) {
    .mypointModal .carousel .carousel-inner .item .carousel-caption {
        right: 15%;
        left: 15%;
    }
}
.mypointModal .carousel .carousel-inner .item .carousel-caption{color: #666666;text-shadow: none;}
.mypointModal .carousel .carousel-inner .item .carousel-caption>h4{line-height: 30px; margin-bottom: 20px;}
.mypointModal .carousel .carousel-control{color: #adadad;}
.mypointModal .carousel .carousel-control:focus,.mypointModal .carousel-control:hover{color: #fff;}
.mypointModal .carousel .carousel-control{background-image:none}


/* login */
.login_main{ width:100%; height:auto;background:url(../images/main_bg.jpg) center top no-repeat;}
.login_main1{ background:url(../images/login_img.png) left bottom no-repeat; min-height:720px; position:relative;}
.login_main1 .login_box{background:#fff;position: absolute; z-index:1; right:0px; top:80px;width: 446px;box-shadow: 0px 0px 7px 0px rgba( 0, 0, 0 , .2);padding:30px 60px;}
.login_box{ padding:20px 30px 0px;-webkit-transition:-webkit-transform .7s ease,visibility .7s ease,z-index .7s ease;-moz-transition:-moz-transform .7s ease,visibility .7s ease,z-index .7s ease;-o-transition:-o-transform .7s ease,visibility .7s ease,z-index .7s ease;transition:transform .7s ease,visibility .7s ease,z-index .7s ease}
.login_box .title{ width:100%; height:auto; overflow:hidden; padding-bottom:10px; font-size:30px;}
.login_box .title a{ color:#1661b9; font-size:18px; line-height:30px;}
.login_box .input-group-addon,.login .form-control{ border-radius:0px; background:none}
.login_box .input-group-addon .fa{ color:#acacac; font-size:20px;}
.login_box .form-control{ height:48px;}
.login_box .btn-block{ height:48px; font-size:20px; border-radius:0px; margin-bottom:10px;}
.login_box .login_type span{ padding:0 5px 0 0; font-size:18px;}
.login_box .login_type a{ margin:0px 10px;}
@media (max-width: 750px) {.login_main1 .login_box{ width:auto; padding:30px; right:10px; left:10px;}}
.modal-dialog-login{ margin-top:250px; width:350px; font-size:16px;}
.login_box.register,.login_box.active{transform: rotateY(90deg);-ms-transform: rotateY(90deg);-moz-transform: rotateY(90deg);-webkit-transform: rotateY(90deg);-o-transform: rotateY(90deg); z-index: -1; visibility: hidden;}
.login_box.register.active { z-index: 11; visibility: visible; display: block; transform: rotateY(0deg); -ms-transform: rotateY(0deg); -moz-transform: rotateY(0deg); -webkit-transform: rotateY(0deg); -o-transform: rotateY(0deg); transition-delay: .5s; -moz-transition-delay: .5s; -webkit-transition-delay: .5s; -o-transition-delay: .5s; }
.login-delay { transition-delay: .5s; -moz-transition-delay: .5s; -webkit-transition-delay: .5s; -o-transition-delay: .5s; }

/** 学习计划-计划展示 **/
.myplan_detail_desc{}
.p_t30_l27{padding: 30px 27px;}
.myplan_detail_desc .pic{border: 1px solid #e6e6e6;}
.myplan_detail_desc p{line-height: 30px;}
.myplan_detail_desc p.title{font-size: 24px;color: #333333;}
.m_t120{margin-top: 120px;}
.myplan_detail_desc .progress{border-radius: 0px;}
.myplan_detail_desc .progress .progress-bar{background-color: #1660b7;}

.myplan_detail_projDir{}
.myplan_detail_projDir .head{font-size: 18px;color: #333333;padding-bottom: 10px;border-bottom: 1px solid #ddd; margin-bottom: 20px;}
.myplan_detail_projDir .panel {border-radius: 0px;border: none; -webkit-box-shadow:none; box-shadow:none;}
.myplan_detail_projDir .panel .panel-heading{border-top-left-radius: 0px;border-top-right-radius: 0px;}
.myplan_detail_projDir .panel.finished .panel-heading{ background-color: #a8a8a8;}
.myplan_detail_projDir .panel.unfinished .panel-heading{ background-color: #5992d5;}
.myplan_detail_projDir .panel .panel-heading a{display: block;color: #FFFFFF;background: url(../images/ico-arrow-up.png) right no-repeat;text-decoration:none;}
.myplan_detail_projDir .panel .panel-heading a.collapsed{background: url(../images/ico-arrow-down.png) right no-repeat;}
.myplan_detail_projDir .panel .panel-body>div{border-left: 3px solid #f1f1f1;padding: 0px 20px;}
.myplan_detail_projDir .panel .panel-body>div>div{border: 1px solid #e6e6e6; margin-left: 0px; margin-right: 0px;}
.myplan_detail_projDir .panel .panel-body>div>div>div{margin: 10px;border-bottom: 1px solid #e6e6e6;padding-bottom: 15px;padding-top: 5px;}
.myplan_detail_projDir .panel .panel-body>div>div>div:last-child{border-bottom: none;}
.myplan_detail_projDir .panel .panel-body>div>div>div>p{font-size: 16px;color: #666666;margin-top: 6px;}
.myplan_detail_projDir .panel .panel-body .item_finished button{background-color: #a8a8a8;color: #fff;}
.myplan_detail_projDir .panel .panel-body .item_unfinished button{background-color: #1660b7;color: #fff;}
.myplan_detail_projDir .panel .panel-body a{margin-top: 6px; margin-left: 10px;}

.myplan_detail_projDir .panel .panel-body>div>div>div.ico-arrow{background: url(../images/ico-arrow-up-xs.png) no-repeat;position: relative; top: -7px; left: 7px; padding-top: 7px;margin: auto;border-bottom:none;padding-bottom: 0px;}















































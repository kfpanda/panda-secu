.top_user{ margin-right: 0; float: right !important; margin: 0!important;}
.top_user .dropdown-toggle{ color: #1660b7; font-size: 18px; line-height: 34px; padding:15px; font-family:microsoft yahei;}
.top_user .dropdown-toggle img{ float: left; width: 35px; height: 35px; margin-right: 8px;}
.top_user li.open a,.top .top_user .dropdown-toggle:hover,.top .top_user .dropdown-toggle:focus{ background-color: #fff;  padding-top:15px; }
.top_user>li>a:focus, .top_user>li>a:hover,.top_user .open>a, .top_user .open>a:focus, .top_user .open>a:hover{background:#fff;}
.top_user li>.dropdown-menu-log{border-radius: 0; border:0; padding: 0!important; position:absolute!important; background:#fff!important;}
.top_user li>.dropdown-menu-log>li>a{padding: 15px 0 15px 20px; font-size: 18px; font-family:microsoft yahei; color: #666666;border-top:0;}
.top_user li>.dropdown-menu-log>li>a i{ width: 40px; float: left; font-size: 20px; text-align: center;}
.top_user li>.dropdown-menu-log .divider{ margin: 0!important;background-color:#ddd;}
.top_user li>.dropdown-menu-log>li>a:focus, .top .top_user li>.dropdown-menu-log>li>a:hover{color:#1660b7;}
.top_user li>.dropdown-menu-log .divider{margin:0;}
.top_user li>.dropdown-menu-log>li>a:focus,.top_user li>.dropdown-menu-log>li>a:hover{color:#be0008;}
@media (max-width: 767px){
    .top_user{padding-top:0;}
}


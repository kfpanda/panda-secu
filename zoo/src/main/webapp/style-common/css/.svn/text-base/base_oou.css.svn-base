.header{ width:100%; height:125px;}
.header .logo{ padding:30px 0px 0px 30px;}
.header .logo1{ padding:45px 0px 0px 40px;}
.header .header_right p.header_link{ margin-top:50px; }
.header .header_right p.header_link a{ color:#333333; font-size:18px; margin-left:20px;}
.header .header_right .header_search{ border:2px solid #ca2f36; color:#ca2f36; border-radius:20px; padding-right:0px; font-size:0px; padding-left:10px; margin-top:40px;}
.header .header_right .header_search i{ font-size:20px; margin:6px 0px; float:left;}
.header .header_right .header_search .form-control{ border:0px none; background:none;}
.header .header_right .header_search .form-control:focus{ box-shadow:none !important}
.header .header_right .header_search .btn{ background:#ca2f36; color:#fff; border-radius:0px; border-top-right-radius:15px; border-bottom-right-radius:15px;}


.top_user{margin-right:0; float: right !important; margin: 0!important; padding-top:29px;}
.top_user .dropdown-toggle{ color: #ca2f36; font-size: 18px; line-height: 34px; padding:15px; font-family:microsoft yahei;}
.top_user .dropdown-toggle img{ float: left; width: 35px; height: 35px; margin-right: 8px;}
.top_user li.open a,.top .top_user .dropdown-toggle:hover,.top .top_user .dropdown-toggle:focus{ background-color: #fff;  padding-top:15px; font-family:microsoft yahei;}
.top_user>li>a:focus, .top_user>li>a:hover,.top_user .open>a, .top_user .open>a:focus, .top_user .open>a:hover{background:#fff;}
.top_user li>.dropdown-menu-log{border-radius: 0; border:0; padding: 0!important; position:absolute!important; background:#fff!important;}
.top_user li>.dropdown-menu-log>li>a{padding: 15px 0 15px 20px; font-size: 18px; font-family:microsoft yahei; color: #666666;border-top:0;}
.top_user li>.dropdown-menu-log>li>a i{ width: 40px; float: left; font-size: 20px; text-align: center;}
.top_user li>.dropdown-menu-log .divider{ margin: 0!important;background-color:#ddd;}
.top_user li>.dropdown-menu-log>li>a:focus, .top .top_user li>.dropdown-menu-log>li>a:hover{color:#1660b7;}
.top_user li>.dropdown-menu-log .divider{margin:0;}
.top_user li>.dropdown-menu-log>li>a:focus,.top_user li>.dropdown-menu-log>li>a:hover{color:#be0008;}
@media (max-width: 767px){
	.top_user{padding-top:0;}
	.top_user .dropdown-toggle{color:#fff;}
	.top_user>li>a:focus, .top_user>li>a:hover,.top_user .open>a, .top_user .open>a:focus, .top_user .open>a:hover{background:#BE0008;}
	.top_user{padding-top:0; margin-top:-5px!important;}
}

/*---------- global ----------*/
body { font: normal 100 14px "microsoft YaHei",SimSun,Arial;}
dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, p, blockquote, tr, td, th, thead, caption { margin: 0; padding:0px; font-weight: normal; list-style:none}


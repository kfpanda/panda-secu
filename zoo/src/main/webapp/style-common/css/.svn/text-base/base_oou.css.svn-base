.index_list1 a .tj:before{ content:""; position:absolute; top:34px; right:0px;z-index:2;display: inline-block;width: 0;height: 0;border-top: 6px dashed;border-top: 6px solid\9;border-right: 6px solid transparent;border-bottom: 6px solid transparent; color:#ca2f36}

.footer{ width:100%; height:auto; overflow:hidden; padding:20px 0px 30px; margin-top:20px; background:#f2f2f2; text-align:center; color:#666666; font:normal 14px/30px "microsoft YaHei";}

.right_nav {position:fixed;right:25px;bottom:25px; width:60px;z-index:999;_position:absolute; _top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)+300||0))); border:1px solid #ebc9c9; border-bottom:0px none;}
.right_nav li{ width:100%;overflow:hidden; border-bottom:1px solid #ebc9c9; background:#f3f3f3;}
.right_nav li a{ display: block;background:#f3f3f3;height:60px;text-decoration: none; color:#666666; font:normal 20px/65px "microsoft YaHei"; text-align:center; position:relative;}
.right_nav li a:hover,.right_nav li.current a{color: #fff; background: #ca1111;}
.right_nav li a span{color: #fff; background: #ca1111;position:absolute; z-index:2; left:0px; top:0px; width:100%; height:60px;font-size:16px; line-height:20px; padding-top:10px; display:none;}
.right_nav .current a span,.right_nav li a:hover span{ display:block;}
.right_nav .r_n_e{height:60px; cursor:pointer;text-indent:-9999px;background:#f3f3f3 url(../images/img_oou/ico_gotop.png) center center no-repeat;}
.right_nav .r_n_e.current{ background:#ca1111 url(../images/img_oou/ico_gotop1.png) center center no-repeat;}


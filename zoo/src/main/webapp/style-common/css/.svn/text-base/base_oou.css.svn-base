.index_link1 span{ font-size:18px; display:block; margin:0 auto; padding-top:10px;}

.index_link2 h5.title{ width:100%; height:auto; overflow:hidden; margin-top:30px; color:#333333; font:normal 18px/30px "microsoft YaHei";}

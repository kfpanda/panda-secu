.index_list5 ul li span{ float:right; color:#333333;font:normal 18px/40px "microsoft YaHei";}

.index_link1{ padding:40px 0px; text-align:center;}
.index_link1 a{ text-decoration:none;color:#333333;}
.index_link1 a:hover{color:#ca2f36}
.index_link1 .fa{ display:block; margin:0 auto; width:150px; height:150px; border-radius:75px; color:#fff; font-size:80px; text-align:center; line-height:150px;}

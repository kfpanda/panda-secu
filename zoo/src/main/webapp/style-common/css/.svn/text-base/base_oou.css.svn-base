.top_gg{ width:100%; height:200px; margin:0px;position:relative; z-index:1; background:url(../images/img_oou/top_imgbg.jpg) left bottom repeat-x; text-align:center;}
.top_gg .img{ position:absolute; text-align:center; left:0px; right:0px; bottom:60px;}
.top_gg_but{ position:absolute; z-index:2; bottom:-30px; left:50%; background:url(../images/img_oou/ico_topimg_but.png) center bottom no-repeat; height:30px; width:50px; margin-left:-25px; cursor:pointer; display:block;}
.top_gg_but:hover{-webkit-animation-duration: 2s;-webkit-animation-name: ticktock;-webkit-animation-iteration-count: infinite;-webkit-animation-timing-function: cubic-bezier(0.645, 0.045, 0.355, 1.000);-webkit-animation-direction: alternate;-webkit-animation-fill-mode: both;-webkit-animation-play-state: running;-webkit-transform-origin: 50% -70%;}


@-webkit-keyframes ticktock {
	0% {-webkit-transform: rotate(0deg)}
	33% {-webkit-transform: rotate(25deg)}
	66% {-webkit-transform: rotate(-25deg)}
	100% {-webkit-transform: rotate(0deg)}
}
@media (max-width: 1200px){
	.top_gg .img{ bottom:70px;}
	.top_gg .img img{ width:80%; height:auto;}

.navbar-collapse{ padding:0px !important}
.navbar{border:0px; background:url(../images/img_oou/ico_nav_bg.jpg) left bottom repeat-x;border-radius:0px; margin-bottom:1px;}
.navbar .navbar-navlist li a{ color:#333333; font-size:24px; font-weight:normal; padding-left:30px;padding-right:30px; border-bottom:4px solid transparent}
.navbar .navbar-navlist li.active a,.navbar .navbar-nav li a:hover,.navbar .navbar-nav li a:focus,.navbar .navbar-nav li.open a{color:#be0008; border-bottom:4px solid #be0008; background:none;}
.navbar-navlist .pull-right a{ color:#666666; font-size:20px; line-height:50px; text-decoration:none}
.navbar-navlist>li>.dropdown-menu{ background:#fff !important; border:0px none !important; border-radius:0px}
.navbar-navlist>li>.dropdown-menu li a{ font-size:16px;border-bottom:0px none !important; line-height:30px;}
.navbar-navlist>li>.dropdown-menu li a:hover{ border-bottom:0px none;}

.carousel .container{ position:relative; z-index:3;}
.carousel .container .banner_join{ position:absolute; z-index:4; right:0px; top:85px; width:auto; min-width:350px;padding:20px 20px 20px 30px; background:url(../images/img_oou/ico_banner_bg.png) repeat}
.banner_join p{ color:#ffffff; font-size:18px; line-height:40px; width:100%; overflow:hidden;}
.banner_join p.num{ height:88px; overflow:hidden; line-height:158px;}
.banner_join p.num span{ display:block; float:left; color:#be0008; font:normal 60px/88px Arial, Helvetica, sans-serif; background:#fff;overflow:hidden; width:60px; margin-right:7px; text-align:center; border-radius:5px;}
.banner_join p.but a{ display:block; width:100%; height:45px; background:#be0008; text-align:center; color:#fff; font:normal 18px/45px "microsoft YaHei"; text-decoration:none; margin-top:10px;}
.banner_footer{ position:absolute; z-index:2; left:0px; bottom:0px; right:0px; background:url(../images/img_oou/banner_footer.png) center bottom no-repeat; height:30px;}

.carousel-indicators li{ border-color:#777; background:#777}
.carousel-indicators li.active{background:#f00;border-color:#f00;}

.index_title{ width:100%; height:110px; padding-top:30px;border-bottom:1px solid #ccc; text-align:center; position:relative;}
.index_title .t1{ color:#333333; font-size:36px;}
.index_title .t2{ color:#333333; font-size:18px; line-height:38px; border-bottom:4px solid #ca2f36}

.index_title1{ width:100%; height:auto; padding:30px 0px;}
.index_title1 span{ color:#333333; font-size:30px; padding-right:20px;}
.index_title1 small{ color:#666666; font-size:18px}
.index_title1 small i{ color:#be0008; padding:0px 5px;font-style: normal;}

.img{overflow:hidden;}
a .img img{-webkit-transition: all ease .3s;transition: all ease .3s}
a:hover .img img{-webkit-transform: scale(1.25);transform: scale(1.25);}

